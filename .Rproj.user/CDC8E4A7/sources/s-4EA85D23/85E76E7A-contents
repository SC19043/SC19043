\name{est.knn}
\alias{est.knn}
\title{KNN density estimator}
\usage{
est.knn(x, k)
}
\arguments{
\item{x}{the data from which the estimate is to be computed.}

\item{k}{the maximum number of nearest neighbors to search, usually close to k^-0.5.}
}
\value{
the estimated density function.
}
\description{
KNN density estimator
}
\examples{
\dontrun{
x<-rnorm(1000)
fhat<-est.knn(x,32)
t<-seq(min(x),max(x),length=200)
plot(t,fhat(t),type="l")
}
}
